/*End of type category*/

SELECT MIN(PP)
FROM Pokemon_Moves;

SELECT DISTINCT Name
FROM Pokemon_Moves
WHERE PP='1';

SELECT MAX(PP)
FROM Pokemon_Moves;

SELECT DISTINCT Name
FROM Pokemon_Moves
WHERE PP='40';


SELECT COUNT(Type)
FROM Pokemon_Moves
WHERE Category='Physical' AND Type='Normal';



