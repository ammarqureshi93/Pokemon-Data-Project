/*Alternative method*/

SELECT TOP 3 Type,Category
FROM Pokemon_Moves
WHERE Category ='Physical';

SELECT TOP 3 Type,Category
FROM Pokemon_Moves
WHERE Category ='Special';

SELECT TOP 3 Type,Category
FROM Pokemon_Moves
WHERE Category ='Status';

/*Finding highest and lowest power*/

SELECT Power,Name
FROM Pokemon_Moves
ORDER By Power DESC;

SELECT Power,Name
FROM Pokemon_Moves
ORDER By Power ASC;

/*Finding Pokemon moves with the highest accuracy and lowest accuracy*/

SELECT Accuracy
FROM Pokemon_Moves
WHERE Accuracy='100%';

SELECT Accuracy
FROM Pokemon_Moves
WHERE Accuracy='0%';
